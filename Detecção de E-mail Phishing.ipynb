   "source": [
    "#Colocar grafico em ordem descrescente e mudar cor\n",
    "FreqMedia = np.zeros(shape=(48))\n",
    "for i in range(48):\n",
    "    word = Phis.columns[i]\n",
    "    FreqMedia[i] = Phis[word].mean()\n",
    "    \n",
    "plt.figure(figsize=(10,5))\n",
    "plt.title(\"Frequência Média de Cada Palavra\")\n",
    "plt.bar(Phis.columns[:48], FreqMedia, color='blue')\n",
    "plt.xticks(Phis.columns[:48], rotation='vertical')\n",
    "plt.savefig('freq.png')\n",
    "plt.plot()\n",
